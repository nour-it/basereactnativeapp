{
  "expo": {
    "name": "Base App",
    "slug": "ractnativebaseapp",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/app-icon.png",
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    },
    "splash": {
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain",
      "image": "./assets/image/splash.png"
    },
    "ios": {
      "icon": "./assets/app-icon.png",
      "bundleIdentifier": "com.nourit.reactnativebaseapp"
    },
    "android": {
      "icon": "./assets/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundImage": "./assets/app-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nourit.reactnativebaseapp",
      "permissions": [
        "RECORD_AUDIO", "RECEIVE_BOOT_COMPLETED", "SCHEDULE_EXACT_ALARM"
      ]
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/app-icon.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ]
  }
}